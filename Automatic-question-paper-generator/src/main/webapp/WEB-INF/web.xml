<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<servlet>
		<servlet-name>userController</servlet-name>
		<display-name>userController</display-name>
		<description></description>
		<servlet-class>com.easyPaper.Controller.userController</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>userLogout</servlet-name>
		<display-name>userLogout</display-name>
		<description></description>
		<servlet-class>com.easyPaper.Controller.userLogout</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>signUpController</servlet-name>
		<display-name>signUpController</display-name>
		<description></description>
		<servlet-class>com.easyPaper.Controller.signUpController</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>classController</servlet-name>
		<display-name>classController</display-name>
		<description></description>
		<servlet-class>com.easyPaper.Controller.classController</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>paperController</servlet-name>
		<display-name>paperController</display-name>
		<description></description>
		<servlet-class>com.easyPaper.Controller.paperController</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>addQuestionController</servlet-name>
		<display-name>addQuestionController</display-name>
		<description></description>
		<servlet-class>com.easyPaper.Controller.addQuestionController</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>contactController</servlet-name>
		<display-name>contactController</display-name>
		<description></description>
		<servlet-class>com.easyPaper.Controller.contactController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>userController</servlet-name>
		<url-pattern>/getUser</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>userLogout</servlet-name>
		<url-pattern>/logout</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>signUpController</servlet-name>
		<url-pattern>/signUp</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>classController</servlet-name>
		<url-pattern>/class</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>paperController</servlet-name>
		<url-pattern>/paper</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>addQuestionController</servlet-name>
		<url-pattern>/question</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>contactController</servlet-name>
		<url-pattern>/contactUs</url-pattern>
	</servlet-mapping>

</web-app>
